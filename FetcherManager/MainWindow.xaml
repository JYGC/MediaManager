<Window x:Class="FetcherManager.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:Videos="clr-namespace:FetcherManager.Tabs.Videos"
        xmlns:Channels="clr-namespace:FetcherManager.Tabs.Channels"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="MainWindow" Height="750" Width="1200">
    <TabControl VerticalContentAlignment="Stretch" HorizontalContentAlignment="Stretch">
        <TabItem Header="Videos">
            <Videos:UCVideos VerticalContentAlignment="Stretch" HorizontalContentAlignment="Stretch" />
        </TabItem>
        <TabItem Header="Channels">
            <Channels:UCChannels VerticalContentAlignment="Stretch" HorizontalContentAlignment="Stretch" />
        </TabItem>
    </TabControl>
</Window>
