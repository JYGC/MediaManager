<UserControl x:Class="FetcherManager.Tabs.Videos.UCVideos"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:Subtabs="clr-namespace:FetcherManager.Tabs.Videos.Subtabs"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <TabControl>
        <TabItem Header="New Videos">
            <Subtabs:UCNewVideos />
        </TabItem>
        <TabItem Header="Get Single Video">
            <Subtabs:UCGetSingleVideo />
        </TabItem>
        <TabItem Header="Download Queue">
            <Subtabs:UCToDownload />
        </TabItem>
        <TabItem Header="Downloaded Videos">
            <Subtabs:UCDownloadedVideos />
        </TabItem>
        <TabItem Header="Ignored Videos">
            <Subtabs:UCIgnoredVideos />
        </TabItem>
    </TabControl>
</UserControl>
