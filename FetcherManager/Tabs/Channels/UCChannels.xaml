<UserControl x:Class="FetcherManager.Tabs.Channels.UCChannels"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:FetcherManager.Tabs.Channels" 
             xmlns:Subtabs="clr-namespace:FetcherManager.Tabs.Channels.Subtabs"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <TabControl>
        <TabItem Header="All Channels">
            <Subtabs:UCAllChannels />
        </TabItem>
        <TabItem Header="Add Channels">
            <Subtabs:UCAddChannels />
        </TabItem>
    </TabControl>
</UserControl>
